	<portlet>
		<portlet-name>wicket7myPortlet</portlet-name>
		<display-name>WICKET 7 MY PORTLET</display-name>
		<portlet-class>org.apache.wicket.portlet.WicketPortlet</portlet-class>
		<init-param>
			<name>wicketFilterPath</name>
			<value>/wicket7myPortlet</value>
		</init-param>
		<init-param>
			<name>check-auth-token</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<supports>
			<mime-type>text/xml</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<supports>
			<mime-type>text/css</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>WICKET 7 MY PORTLET</title>
			<short-title>WICKET 7 MY PORTLET</short-title>
			<keywords>WICKET 7 MY PORTLET</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
	
</portlet-app>